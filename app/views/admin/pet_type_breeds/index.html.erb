  </tr>

<% @pet_type_breeds.each do |pet_type_breed| %>
  <tr>
    <td><%= pet_type_breed.raza %></td>
    <td><%= pet_type_breed.pet_type.PetTypeName %></td>
    <td><% link_to 'Show', admin_pet_type_breed_path(pet_type_breed) %></td>
    <td><%= link_to 'Editar', edit_admin_pet_type_breed_path(pet_type_breed) %></td>
    <td><%= link_to 'Borrar', admin_pet_type_breed_path(pet_type_breed), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

