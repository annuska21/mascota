<!DOCTYPE html>
<html>
<head>
  <title>Mascota</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <% if !flash[:notice].blank? %>
        <div class="notice">
          <%= flash[:notice] %>
       </div>
    <% end %>
 <div class="container">
     <%= render 'layouts/header' %>
    <%= yield %>
    <%= render 'layouts/footer' %>
 </div>
</body>
</html>
